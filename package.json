{
  "name": "@kulmann/vuetable-2",
  "version": "1.7.6",
  "description": "FORKED - Datatable component for Vue 2.x",
  "main": "dist/vuetable-2.js",
  "license": "MIT",
  "author": "Benedikt Kulmann <b@kulmann.biz>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kulmann/vuetable-2.git"
  },
  "private": false,
  "keywords": [
    "vue",
    "component",
    "table",
    "datatable",
    "pagination-components",
    "json"
  ],
  "browserify": {
    "transform": [
      "babelify",
      [
        "vueify",
        {
          "_flags": {
            "debug": true
          }
        }
      ]
    ]
  },
  "scripts": {
    "dev": "node build/dev-server.js",
    "start": "node build/dev-server.js",
    "build": "node build/build.js && npm run bundle",
    "bundle:min": "cross-env MINIFY=true webpack --progress --config build/webpack.build.config.js",
    "bundle:full": "cross-env MINIFY=false webpack --progress --config build/webpack.build.config.js",
    "bundle": "npm run bundle:min && npm run bundle:full",
    "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
    "e2e": "node test/e2e/runner.js",
    "test": "npm run unit && npm run e2e"
  },
  "dependencies": {
    "axios": "^0.15.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^6.2.10",
    "babel-plugin-istanbul": "^4.1.6",
    "babel-plugin-transform-runtime": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-stage-2": "^6.22.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "babelify": "^7.3.0",
    "chai": "^3.5.0",
    "chalk": "^1.1.3",
    "connect-history-api-fallback": "^1.6.0",
    "copy-webpack-plugin": "^4.6.0",
    "cross-env": "^4.0.0",
    "cross-spawn": "^5.0.1",
    "css-loader": "^0.28.11",
    "eventsource-polyfill": "^0.9.6",
    "express": "^4.17.1",
    "extract-text-webpack-plugin": "^2.0.0",
    "file-loader": "^0.11.1",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^2.30.1",
    "http-proxy-middleware": "^0.17.3",
    "inject-loader": "^3.0.1",
    "isparta-loader": "^2.0.0",
    "json-loader": "^0.5.7",
    "karma": "^1.7.1",
    "karma-coverage": "^1.1.2",
    "karma-mocha": "^1.3.0",
    "karma-phantomjs-launcher": "^1.0.2",
    "karma-phantomjs-shim": "^1.5.0",
    "karma-sinon-chai": "^1.3.4",
    "karma-sourcemap-loader": "^0.3.8",
    "karma-spec-reporter": "0.0.30",
    "karma-webpack": "^2.0.13",
    "lolex": "^1.5.2",
    "mocha": "^3.5.3",
    "nightwatch": "^0.9.21",
    "opn": "^4.0.2",
    "optimize-css-assets-webpack-plugin": "^1.3.0",
    "ora": "^1.4.0",
    "phantomjs-prebuilt": "^2.1.16",
    "promise-polyfill": "^6.1.0",
    "rimraf": "^2.7.1",
    "selenium-server": "^3.141.59",
    "semver": "^5.7.1",
    "shelljs": "^0.7.6",
    "sinon": "^2.4.1",
    "sinon-chai": "^2.14.0",
    "stats-webpack-plugin": "0.4.3",
    "url-loader": "^0.5.8",
    "vue": "^2.5.2",
    "vue-hot-reload-api": "^1.2.0",
    "vue-html-loader": "1.2.3",
    "vue-loader": "^12.2.2",
    "vue-style-loader": "^3.1.2",
    "vue-template-compiler": "^2.5.2",
    "vueify": "^9.4.1",
    "webpack": "^2.7.0",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-dev-middleware": "^1.12.2",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^4.2.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
